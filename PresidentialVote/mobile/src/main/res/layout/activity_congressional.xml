        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:layout_alignTop="@+id/house_rep"
        android:layout_alignStart="@+id/textView14" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/democrat"
        android:id="@+id/party_3"
        android:layout_alignParentEnd="true"
        android:paddingLeft="45dp"
        android:layout_below="@+id/name_3"
        android:layout_alignLeft="@+id/textView14" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/email_3"
        android:id="@+id/email_3"
        android:layout_alignParentEnd="true"
        android:paddingLeft="45dp"
        android:layout_below="@+id/party_3"
        android:layout_alignLeft="@+id/textView14" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/website_3"
        android:id="@+id/website_3"
        android:paddingLeft="45dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/email_3"
        android:layout_alignLeft="@+id/textView14" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tweet_3"
        android:id="@+id/tweet_3"
        android:paddingLeft="45dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/website_3"
        android:layout_alignLeft="@+id/textView14" />
</RelativeLayout>

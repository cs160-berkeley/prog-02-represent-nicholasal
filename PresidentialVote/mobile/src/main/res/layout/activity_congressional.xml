        android:paddingTop="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/senator_2"
        android:id="@+id/name_2"
        android:paddingLeft="15dp"
        android:layout_below="@+id/line2"
        android:layout_alignStart="@+id/tweet_1"
        android:paddingTop="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/democrat"
        android:id="@+id/party_2"
        android:layout_alignParentEnd="true"
        android:paddingLeft="45dp"
        android:layout_below="@+id/name_2"
        android:layout_alignLeft="@+id/tweet_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/email_2"
        android:id="@+id/email_2"
        android:layout_alignParentEnd="true"
        android:paddingLeft="45dp"
        android:layout_below="@+id/party_2"
        android:layout_alignLeft="@+id/tweet_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/website_2"
        android:id="@+id/website_2"
        android:paddingLeft="45dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/email_2"
        android:layout_alignLeft="@+id/tweet_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tweet_2"
        android:id="@+id/tweet_2"
        android:paddingLeft="45dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/website_2"
        android:layout_alignLeft="@+id/tweet_1" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:layout_below="@+id/tweet_2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="15dp"
        android:id="@+id/line3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/result2"
        android:id="@+id/textView14"
        android:textSize="18dp"
        android:paddingTop="3dp"
        android:layout_alignTop="@+id/line3"
        android:layout_toEndOf="@+id/textView15"
        android:paddingLeft="10dp"
        android:layout_alignStart="@+id/tweet_2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="    1    "
        android:id="@+id/textView15"
        android:visibility="visible"
        android:textSize="18dp"
        android:paddingTop="3dp"
        android:layout_alignTop="@+id/line3"
        android:layout_alignParentStart="true"
        android:paddingLeft="10dp" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:layout_below="@+id/textView14"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:id="@+id/line4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/house_rep"
        android:src="@drawable/mark"
        android:scaleType="fitCenter"
        android:background="#ffffff"
        android:clickable="true"
        android:layout_below="@+id/line4"
        android:layout_marginBottom="109dp"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/senator_2"
        android:paddingTop="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/house_rep"

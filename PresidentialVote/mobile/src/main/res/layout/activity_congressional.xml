    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/democrat"
        android:id="@+id/party_1"
        android:layout_alignStart="@+id/name_1"
        android:layout_below="@+id/name_1"
        android:layout_alignParentEnd="true"
        android:paddingLeft="45dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/email_1"
        android:id="@+id/email_1"
        android:layout_below="@+id/party_1"
        android:layout_alignStart="@+id/party_1"
        android:layout_alignParentEnd="true"
        android:paddingLeft="45dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/website_1"
        android:id="@+id/website_1"
        android:paddingLeft="45dp"
        android:layout_below="@+id/email_1"
        android:layout_alignStart="@+id/email_1"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tweet_1"
        android:id="@+id/tweet_1"
        android:paddingLeft="45dp"
        android:layout_below="@+id/website_1"
        android:layout_alignStart="@+id/website_1"
        android:layout_alignParentEnd="true" />
